modules = ["web", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["cairo", "ffmpeg-full", "freetype", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "pkg-config", "qhull", "tcl", "tk"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "CrewAI Data Analyst"

[[workflows.workflow]]
name = "CrewAI Data Analyst"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python crew.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
